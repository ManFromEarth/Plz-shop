page {
  background-color: #f5f5f5;
  border-top: 1rpx #e5e5e5 solid;
  box-sizing: border-box;
  font-size: 30rpx;
}

.cartView {
  height: 100%;
}

.blank {
  width: 100%;
  height: 100%;
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
}

.activePost {
  height: 88rpx;
  display: flex;
  align-items: center;
  background-color: #fff;
  justify-content: center;
}

.adoptwrap {
  height: 300rpx;
  position: relative;
}

.adopt {
  position: absolute;
  width: 300rpx;
  height: 80rpx;
  background-color: #d3d3d3;
  bottom: 24rpx;
  right: 56rpx;
  display: flex;
  font-size: 30rpx;
  align-items: center;
  justify-content: space-around;
}

.delivery {
  border-bottom: 1rpx #e5e5e5 solid;
  height: 90rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  justify-content: space-between;
}

.detailadress {
  font-size: 24rpx;
}

.positiondel {
  display: flex;
  flex: 1;
}

.postcost {
  font-size: 28rpx;
  display: flex;
  align-items: center;
}

.productwrap {
  background-color: #fff;
}

.singelP {
  padding: 0 20rpx;
  height: 210rpx;
  display: flex;
  border-bottom: 1rpx #e5e5e5 solid;
  align-items: center;
  position: relative;
}

.iName {
  font-size: 30rpx;
  color: #383838;
}

.iDetail {
  font-size: 30rpx;
  color: #969696;
  margin-top: 10rpx;
}

.iTips {
  display: flex;
  font-size: 20rpx;
  margin-top: 14rpx;
  color: #969696;
}

.iTipscell {
  padding: 4rpx;
  border-radius: 8rpx;
  border: 1rpx #ff4a91 solid;
  margin-right: 10rpx;
}

.specs {
  display: flex;
  margin-top: 4rpx;
  align-items: baseline;
}

.specs-desc {
  margin-left: 6rpx;
  line-height: 24rpx;
}

.iPrice {
  display: flex;
  color: #ff4a91;
  align-items: flex-end;
}

.priceL {
  font-size: 22rpx;
  margin-right: 8rpx;
  align-self: flex-end;
}

.priceR {
  font-size: 34rpx;
  line-height: 34rpx;
  margin-bottom: 4rpx;
}

.cart {
  position: absolute;
  right: 0;
  bottom: 30rpx;
}

.hasthing {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 148rpx;
  position: absolute;
  right: 20rpx;
  bottom: 30rpx;
}

.accounts {
  position: fixed;
  bottom: 0;
  height: 96rpx;
  background-color: white;
  width: 100%;
  display: flex;
}

.accountsL {
  border-top: 1rpx #e5e5e5 solid;
  box-sizing: border-box;
  flex: 1;
  display: flex;
  align-items: center;
}

.accountsR {
  background-color: #ff4a91;
  width: 244rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
}

.totalprice {
  font-size: 30rpx;
  margin-left: 20rpx;
}

.exeal {
  font-size: 24rpx;
  color: #ff4991;
}

.redprice {
  color: #ff4a91;
  margin-left: 8rpx;

}

.arrowright {
  font-size: 0;
  line-height: 0;
  border-width: 14rpx;
  border-color: #fff;
  border-right-width: 0;
  border-style: dashed;
  border-left-style: solid;
  border-top-color: transparent;
  border-bottom-color: transparent;
}

.accountcell {
  font-size: 34rpx;
  margin-right: 24rpx;
}

.amount {
  background-color: #fff;
  margin-top: 20rpx;
  padding: 0 20rpx;
  border-bottom: 96rpx #f5f5f5 solid;
}

.amount-cell {
  height: 86rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx #e6e6e6 solid;
}

.amount-cell-no {
  border-bottom: none;
}

.left-cell {
  flex: 1;
}

.gray {
  color: #babbba;
}

.car-blank-wrap {
  width: 140rpx;
  height: 140rpx;
  border: 2rpx #eaeaea solid;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  margin-top: 208rpx;
}

.car-blank-wrap image {
  width: 62rpx;
  height: 62rpx;
}

.car-blank-desc {
  margin-top: 46rpx;
  color: #888;
  font-size: 28rpx;
}

.poisign {
  position: absolute;
}

.poisign::after, .poisign::before {
  border: 10px solid transparent;
  border-left: 10px solid #fff;
  width: 0;
  height: 0;
  position: absolute;
  top: 0;
  right: -2px;
  content: '';
}

.poisign::before {
  border-left-color: #f00;
  right: -4px;
}

.posttxt {
  display: flex;
  margin: 0 12rpx;
  align-items: center;
  font-size: 30rpx;
  line-height: 30rpx;
}

.sel-button {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
  background-color: #f84980;
}

.delivery-r {
  font-size: 24rpx;
  color: #ff68a4;
  display: flex;
}

.acc-allsel {
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
  border: 2rpx #e9e9e9 solid;
  margin-right: 20rpx;
}

.allsel-wrap {
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  margin-right: 40rpx;
}

.labcircle {
  margin-right: 20rpx;
  width: 44rpx;
  height: 44rpx;
  background-color: #f84980;
  border-radius: 50%;
}

.labelId {
  display: inline-block;
  width: 10rpx;
  height: 4rpx;
  background: #fff;
  line-height: 0;
  font-size: 0;
  transform: rotate(45deg);
  margin-left: 10rpx;
}

.labelId:after {
  content: '/';
  display: block;
  width: 20rpx;
  height: 4rpx;
  background: #fff;
  transform: rotate(-90deg) translateY(-50%) translateX(50%);
}

.sinR {
  font-size: 22rpx;
}

.carBaner {
  width: 100%;
  height: 300rpx;
}

.Pcadress {
  margin-left: 6rpx;
}

.pro-cover {
  width: 170rpx;
  height: 170rpx;
  margin-right: 30rpx;
  margin-left: 22rpx;
  border-radius: 6rpx;
}
.wxPayAmount{
  color: #ff4a91;
  font-size: 30rpx;
  line-height: 30rpx;
  margin-left: 6rpx;
}
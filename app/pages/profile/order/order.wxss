}
.product-amount {
}
.UsedCash {
  margin-top: 8rpx;
}

.UsedCoupon {
  margin-top: 8rpx;
}

.control-button {
  padding: 8rpx 10rpx;
  border: 2rpx #ff6953 solid;
  border-radius: 6rpx;
  margin-left: 8rpx;
}

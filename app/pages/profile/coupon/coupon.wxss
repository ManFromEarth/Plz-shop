page {
  background-color: #f0f0f0; 
  padding-top: 30rpx;
  box-sizing: border-box;
  font-size: 26rpx;
}

.coupon {
  background-color: #fff;
  border-radius: 10rpx;
  padding: 40rpx 30rpx;
  margin: 0 28rpx 10rpx 28rpx;
}

.coupon-top {
  display: flex;
}

.dec {
  height: 108rpx;
}

.sum {
  display: flex;
  color: #ff4a91;
  margin-right: 50rpx;
  width: 100rpx;
}

.symbol {
  margin-top: 30rpx;
  margin-right: 8rpx;
}

.sum-no {
  font-size: 64rpx;
}

.dashed {
  height: 1px;
  width: 100%;
  border-bottom: 2rpx #e6e6e6 dashed;
}

.full {
  color: #969696;
}

.validity {
  margin-top: 26rpx;
}

.noCouponwrap {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 410rpx;
  color: #828182;
}

.noCoupon {
}

.circleBg {
  height: 180rpx;
  width: 180rpx;
  background-color: #f6f7f8;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  margin-bottom: 80rpx;
}
